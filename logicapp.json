{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "logicAppName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 80,
      "metadata": {
        "description": "Name of the Logic App."
      }
    },
    "logicAppLocation": {
      "type": "string",
      "defaultValue": "eastus2",
      "allowedValues": [
        "[resourceGroup().location]",
        "eastasia",
        "southeastasia",
        "centralus",
        "eastus",
        "eastus2",
        "westus",
        "northcentralus",
        "southcentralus",
        "northeurope",
        "westeurope",
        "japanwest",
        "japaneast",
        "brazilsouth",
        "australiaeast",
        "australiasoutheast",
        "southindia",
        "centralindia",
        "westindia",
        "canadacentral",
        "canadaeast",
        "uksouth",
        "ukwest",
        "westcentralus",
        "westus2",
        "koreacentral",
        "koreasouth",
        "francecentral",
        "francesouth",
        "uaecentral",
        "uaenorth",
        "southafricanorth",
        "southafricawest",
        "switzerlandnorth",
        "switzerlandwest",
        "germanynorth",
        "germanywestcentral",
        "norwayeast",
        "brazilsoutheast"
      ],
      "metadata": {
        "description": "Location of the Logic App."
      }
    },
    "LogicAppIntegrationAccount": {
      "type": "string",
      "minLength": 1,
      "defaultValue": "/subscriptions/1824bal3-123v-4dab-9a84-b0d509ld23c7/resourcegroups/ise/providers/Microsoft.Logic/integrationAccounts/ise"
    },
    "MerchIdURL": {
      "type": "string"
    },
    "CurrentStoresURL": {
      "type": "string"
    },
    "APIUserName": {
      "type": "string"
    },
    "InformationEmailTo": {
      "type": "string"
    },
    "ErrorEmailTo": {
      "type": "string"
    },
    "sftpfolderPath": {
      "type": "string"
    },
    "environment": {
      "type": "string"
    },
    "EncStorageConnection": {
      "type": "string"
    },
    "KeyVaultBaseAddress": {
      "type": "string"
    },
    "secretName": {
      "type": "string"
    },
    "passPhrase": {
      "type": "securestring"
    },
    "sftpwithssh_1_Connection_Name": {
      "type": "string",
      "defaultValue": "sftpwithssh"
    },
    "sftpwithssh_1_Connection_DisplayName": {
      "type": "string",
      "defaultValue": "my-ise"
    },
    "sftpwithssh_1_hostName": {
      "type": "string",
      "metadata": {
        "description": "Host server address"
      },
      "defaultValue": "dasdas.sdsd.com"
    },
    "sftpwithssh_1_userName": {
      "type": "string",
      "metadata": {
        "description": "User name"
      },
      "defaultValue": "major"
    },
    "sftpwithssh_1_password": {
      "type": "securestring",
      "metadata": {
        "description": "Password"
      }
    },
    "sftpwithssh_1_sshPrivateKey": {
      "type": "securestring",
      "metadata": {
        "description": "SSH private key (the content of the file should be provided entirely as is, in the multiline format)"
      }
    },
    "sftpwithssh_1_sshPrivateKeyPassphrase": {
      "type": "securestring",
      "metadata": {
        "description": "SSH private key passphrase (if the private key is protected by a passphrase)"
      }
    },
    "sftpwithssh_1_portNumber": {
      "type": "int",
      "metadata": {
        "description": "SFTP port number (example: 22)"
      },
      "defaultValue": 22
    },
    "sftpwithssh_1_acceptAnySshHostKey": {
      "type": "bool",
      "metadata": {
        "description": "Disable SSH host key validation? (True/False)"
      },
      "defaultValue": true
    },
    "sftpwithssh_1_sshHostKeyFingerprint": {
      "type": "string",
      "metadata": {
        "description": "SSH host key finger-print"
      },
      "defaultValue": ""
    },
    "sftpwithssh_1_rootFolder": {
      "type": "string",
      "metadata": {
        "description": "Root folder path."
      },
      "defaultValue": ""
    },
    "office365_1_Connection_Name": {
      "type": "string",
      "defaultValue": "office365"
    },
    "office365_1_Connection_DisplayName": {
      "type": "string"
    },
    "logicAppISEname": {
      "type": "string",
      "defaultValue": "my-ise"
    },
    "logicAppISEid": {
      "type": "string",
      "defaultValue": "/subscriptions/1824bal3-123v-4dab-9a84-b0d509ld23c7/resourceGroups/rga123/providers/Microsoft.Logic/integrationServiceEnvironments/ise123"
    },
    "azureblob_1_Connection_Name": {
      "type": "string",
      "defaultValue": "azureblob-1"
    },
    "azureblob_1_Connection_DisplayName": {
      "type": "string",
      "defaultValue": "MerchInt"
    },
    "azureblob_1_accountName": {
      "type": "string",
      "metadata": {
        "description": "Name of the storage account the connector should use."
      },
      "defaultValue": "merch"
    },
    "azureblob_1_accessKey": {
      "type": "securestring",
      "metadata": {
        "description": "Specify a valid primary/secondary storage account access key."
      }
    },
    "azureblob_1_privacySetting": {
      "type": "string",
      "metadata": {
        "description": "Privacy Setting"
      },
      "defaultValue": "None",
      "allowedValues": [
        "None",
        "Private",
        "Organizational",
        "Public"
      ]
    }
  },
  "variables": {
    "environmentPrefix": "[concat(first(parameters('environment')))]",
    "logicappfullname": "[concat('eus2', variables('environmentPrefix'), '-la-', parameters('logicAppName'))]"
  },
  "resources": [
    {
      "name": "[variables('logicappfullname')]",
      "type": "Microsoft.Logic/workflows",
      "location": "[parameters('logicAppLocation')]",
      "tags": {
        "displayName": "LogicApp"
      },
      "apiVersion": "2016-06-01",
      "properties": {
        "integrationServiceEnvironment": {
          "name": "[parameters('logicAppISEname')]",
          "id": "[parameters('logicAppISEid')]",
          "type": "Microsoft.Logic/integrationServiceEnvironments"
        },
        "definition": {
          "$schema": "https://schema.management.azure.com/providers/Microsoft.Logic/schemas/2016-06-01/workflowdefinition.json#",
          "actions": {
           
          },
          "parameters": {
            "$connections": {
              "defaultValue": {},
              "type": "Object"
            },
            "MerchIdURL": {
              "type": "string"
            },
            "CurrentStoresURL": {
              "type": "string"
            },
            "APIUserName": {
              "type": "string"
            },
            "APIPassword": {
              "type": "securestring"
            },
            "InformationEmailTo": {
              "type": "string"
            },
            "ErrorEmailTo": {
              "type": "string"
            },
            "sftpfolderPath": {
              "type": "string"
            },
            "environment": {
              "type": "string"
            },
            "KeyVaultBaseAddress": {
              "type": "string"
            },
            "secretName": {
              "type": "string"
            },
            "passPhrase": {
              "type": "securestring"
            },
            "EncStorageConnection": {
              "type": "string"
            },
            "sftpwithssh_1_hostName": {
              "type": "string",
              "metadata": {
                "description": "Host server address"
              }
            },
            "sftpwithssh_1_userName": {
              "type": "string",
              "metadata": {
                "description": "User name"
              }
            }
          },
          "triggers": {
            "Recurrence": {
              "type": "Recurrence",
              "recurrence": {
                "frequency": "Week",
                "interval": 1,
                "timeZone": "Eastern Standard Time",
                "schedule": {
                  "weekDays": [
                    "Sunday",
                    "Wednesday"
                  ],
                  "hours": [
                    "19"
                  ],
                  "minutes": [
                    0
                  ]
                }
              }
            }
          },
          "contentVersion": "1.0.0.0",
          "outputs": {}
        },
        "parameters": {
          "$connections": {
            "value": {
              "sftpwithssh": {
                "id": "[concat(parameters('logicAppISEid'), '/managedApis/sftpwithssh')]",
                "connectionId": "[resourceId('Microsoft.Web/connections', parameters('sftpwithssh_1_Connection_Name'))]",
                "connectionName": "[parameters('sftpwithssh_1_Connection_Name')]"
              },
              "office365": {
                "id": "[concat(subscription().id, '/providers/Microsoft.Web/locations/', parameters('logicAppLocation'), '/managedApis/', 'office365')]",
                "connectionId": "[resourceId('Microsoft.Web/connections', parameters('office365_1_Connection_Name'))]",
                "connectionName": "[parameters('office365_1_Connection_Name')]"
              },
              "azureblob": {
                "id": "[concat(parameters('logicAppISEid'), '/managedApis/azureblob')]",
                "connectionId": "[resourceId('Microsoft.Web/connections', parameters('azureblob_1_Connection_Name'))]",
                "connectionName": "[parameters('azureblob_1_Connection_Name')]"
              }
            }
          }
        },
        "integrationAccount": {
          "id": "[parameters('LogicAppIntegrationAccount')]"
        }
      },
      "dependsOn": [
        "[resourceId('Microsoft.Web/connections', parameters('sftpwithssh_1_Connection_Name'))]",
        "[resourceId('Microsoft.Web/connections', parameters('office365_1_Connection_Name'))]",
        "[resourceId('Microsoft.Web/connections', parameters('azureblob_1_Connection_Name'))]"
      ]
    },
    {
      "type": "MICROSOFT.WEB/CONNECTIONS",
      "apiVersion": "2018-07-01-preview",
      "name": "[parameters('sftpwithssh_1_Connection_Name')]",
      "location": "[parameters('logicAppLocation')]",
      "properties": {
        "api": {
          "id": "[concat(parameters('logicAppISEid'), '/managedApis/sftpwithssh')]"
        },
        "displayName": "[parameters('sftpwithssh_1_Connection_DisplayName')]",
        "parameterValues": {
          "hostName": "[parameters('sftpwithssh_1_hostName')]",
          "userName": "[parameters('sftpwithssh_1_userName')]",
          "password": "[parameters('sftpwithssh_1_password')]",s
          "sshPrivateKey": "[parameters('sftpwithssh_1_sshPrivateKey')]",
          "sshPrivateKeyPassphrase": "[parameters('sftpwithssh_1_sshPrivateKeyPassphrase')]",
          "portNumber": "[parameters('sftpwithssh_1_portNumber')]",
          "acceptAnySshHostKey": "[parameters('sftpwithssh_1_acceptAnySshHostKey')]",
          "sshHostKeyFingerprint": "[parameters('sftpwithssh_1_sshHostKeyFingerprint')]",
          "rootFolder": "[parameters('sftpwithssh_1_rootFolder')]"
        }
      }
    },
    {
      "type": "MICROSOFT.WEB/CONNECTIONS",
      "apiVersion": "2018-07-01-preview",
      "name": "[parameters('office365_1_Connection_Name')]",
      "location": "[parameters('logicAppLocation')]",
      "properties": {
        "api": {
          "id": "[concat(subscription().id, '/providers/Microsoft.Web/locations/', parameters('logicAppLocation'), '/managedApis/', 'office365')]"
        },
        "displayName": "[parameters('office365_1_Connection_DisplayName')]"
      }
    },
    {
      "type": "MICROSOFT.WEB/CONNECTIONS",
      "apiVersion": "2018-07-01-preview",
      "name": "[parameters('azureblob_1_Connection_Name')]",
      "location": "[parameters('logicAppLocation')]",
      "properties": {
        "api": {
          "id": "[concat(parameters('logicAppISEid'), '/managedApis/azureblob')]"
        },
        "displayName": "[parameters('azureblob_1_Connection_DisplayName')]",
        "parameterValues": {
          "accountName": "[parameters('azureblob_1_accountName')]",
          "accessKey": "[parameters('azureblob_1_accessKey')]",
          "privacySetting": "[parameters('azureblob_1_privacySetting')]"
        }
      }
    }
  ],
  "outputs": {
    "logicAppName": {
      "type": "string",
      "value": "[variables('logicappfullname')]"
    },
    "rgName": {
      "type": "string",
      "value": "[resourceGroup().name]"
    },
    "subsName": {
      "type": "string",
      "value": "[subscription().id]"
    },
    "logicAppResourceID": {
      "type": "string",
      "value": "[resourceId('Microsoft.Logic/workflows', variables('logicappfullname'))]"
    }
  }
}